{
  "name": "foxify",
  "version": "0.10.20",
  "description": "The fast, easy to use & typescript ready web framework for Node.js",
  "homepage": "https://foxify.js.org",
  "author": "Ardalan Amini <ardalanamini22@gmail.com> (https://ardalanamini.com)",
  "funding": [
    {
      "type": "opencollective",
      "url": "https://opencollective.com/foxify"
    }
  ],
  "license": "MIT",
  "repository": "github:foxifyjs/foxify",
  "bugs": {
    "url": "https://github.com/foxifyjs/foxify/issues"
  },
  "keywords": [
    "node",
    "fast",
    "speed",
    "typescript",
    "api",
    "web",
    "app",
    "application",
    "route",
    "router",
    "routing",
    "middleware",
    "cluster",
    "clustering",
    "database",
    "model",
    "framework",
    "foxify"
  ],
  "scripts": {
    "prepublishOnly": "pnpm lint && pnpm test && pnpm build",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint src test scripts jest.config.js",
    "lint:fix": "pnpm lint --fix",
    "typedoc": "typedoc --out docs/ --mode file --module commonjs --target ES6 --plugin typedoc-plugin-example-tag src/",
    "doc": "node scripts/doc.js",
    "test": "node --no-compilation-cache --expose-gc node_modules/jest/bin/jest --logHeapUsage",
    "test:coverage": "pnpm test -- --coverage",
    "test:ci": "pnpm test:coverage --ci --verbose --no-cache"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "files": [
    "framework"
  ],
  "main": "framework/index.js",
  "types": "framework/index.d.ts",
  "peerDependencies": {
    "@foxify/http": "^1",
    "@foxify/router": "^1",
    "@types/node": "^14",
    "prototyped.js": "^1"
  },
  "dependencies": {
    "@foxify/inject": "^1.1.0",
    "dotenv": "^16.0.3",
    "escape-html": "^1.0.3",
    "etag": "^1.8.1",
    "fast-json-stringify": "^5.4.0",
    "proxy-addr": "^2.0.7",
    "qs": "^6.11.0",
    "safe-regex": "^2.1.1",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@foxify/http": "^1.0.0-beta.2",
    "@foxify/router": "^1.0.0-beta.2",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/escape-html": "^1.0.2",
    "@types/etag": "^1.8.1",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/proxy-addr": "^2.0.0",
    "@types/qs": "^6.9.7",
    "@types/safe-regex": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.8",
    "esdoc-typescript-plugin": "^1.0.1",
    "eslint": "^8.26.0",
    "eslint-config-noir": "^1.3.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.2.2",
    "morgan": "^1.10.0",
    "prototyped.js": "^1.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.20",
    "typescript": "^4.8.4"
  }
}
